
fn hook_select_compare(hooks: Array[Func], left_type: Value, right_type: Value) ?Func {

    each hooks as func {
        let args = func.args
        let arg1 = args.get(0) ! continue
        let arg2 = args.get(1) ! continue
        if !arg1.type.compat(left_type) : continue
        if !arg2.type.compat(right_type) : continue
        return func
    }

    return null
}