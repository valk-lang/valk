
fn stage_ast(b: Build) {

    if b.verbose > 2 : b.log("> Stage 3: Parse AST")

    each b.units as unit {
        each unit.functions as func {
            parse_func_ast(func)
        }

        stage_unroll(unit)
        stage_ir(unit)
        stage_object(unit)
    }

    stage_link(b)
}


