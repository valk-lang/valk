
fn stage_alias(b: Build) {

    each b.units as unit {
        each unit.aliasses as alias {
            let p = Parser.new(alias.chunk, unit, null)

            if alias.type == alias_type {
                let type = read_type(p)
                alias.scope.identifiers.set(alias.name, Idf.for_type(type))
            }
        }
    }

    stage_props(b)
}