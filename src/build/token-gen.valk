
fn tgen_statement(v: Value) Token {
    return Token {
        type: tk_statement
        value1: v
    }
}

fn tgen_declare(decl: Decl, value: Value) Token {
    return Token {
        type: tk_declare
        decl: decl
        value1: value
    }
}

fn tgen_assign(v1: Value, v2: Value) Token {
    return Token {
        type: tk_assign
        value1: v1
        value2: v2
    }
}