

fn read_generic_names(p: Parser) Array[String] {
    let names = array[String]{}
    while true {
        let name = p.read_word(true, false)
        if names.contains(name) : p.error("Duplicate name for generic type")
        names.push(name)
        p.expect2(",", "]", true, false)
    }
    return names
}
