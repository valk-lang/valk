
test "Map: initialization & get-by-key" {
    // let m = map[String]{ "a" => "1", "b" => "2", "c" => "3" }
    let m = Map[String].new()
    m.set("a", "1")
    m.set("b", "2")
    m.set("c", "3")

    assert((m.get("a") ? "0") == "1")
    assert((m.get("b") ? "0") == "2")
    assert((m.get("c") ? "0") == "3")
    assert((m.get("d") ? "0") == "0")
}
