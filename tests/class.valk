
class ClassTest {
    value: int (5)

    get val : int {
        return this.value + 2
    }
    group sub {
        get val : int {
            return this.value + 20
        }
        static get stat : int {
            return 40
        }
    }
}

test "Class syntax" {
    let x = ClassTest{}
    assert(x.value == 5)
    assert(x.val == 7)
    assert(x.sub.val == 25)
    assert(ClassTest.sub.stat == 40)
}