
test "Math" {
    let a = 2
    let b = 10
    let c = 11
    // Prio
    assert(a + b * c == 112)
    assert(c + b / a == 16)
    // Add
    assert(a + b == 12)
    // Sub
    assert(a - b == -8)
    assert(b - a == 8)
    assert((b - a) != (a - b))
    // Div
    assert(0 / a == 0)
    assert(b / a == 5)
    // Mult
    assert(a * 0 == 0)
    // Shift
    assert(a << 2 == 8)
    assert(b >> 1 == 5)
    assert(b >> 2 == 2)
    // Mod
    assert(b % 2 == 0)
    assert(c % 2 == 1)
    assert(c % 10 == 1)
}
