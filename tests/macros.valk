
test "Macro: array" {
    let arr = array[String]{}
    assert(arr.length == 0)
    arr = array[String]{ "a", "b", "c" }
    assert(arr.length == 3)
    arr = array[String]{
        "a",
        "b"
    }
    assert(arr.length == 2)
    arr = array[String]{
        "a"
        "b",
    }
    assert(arr.length == 2)
}

test "Macro: map" {
    let m = map[String]{}
    assert(m.length() == 0)
    m = map[String]{ "a" => "1",
        "b" => "2"
        "c" => "3",
    }
    assert(m.length() == 3)
    assert((m.get("a") !? "9") == "1")
    assert((m.get("b") !? "9") == "2")
    assert((m.get("c") !? "9") == "3")
}
