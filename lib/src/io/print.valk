
header "sys" as sys

fn println(msg: String) {
    let data = msg.data_ptr()
    let len : uint = msg.bytes
    let fd : i32 = 1
    sys:write(fd, data, len)
    sys:write(fd, "\n".data_ptr(), 1)
}

fn print(msg: String) {
    let data = msg.data_ptr()
    let len : uint = msg.bytes
    let fd : i32 = 1
    sys:write(fd, data, len)
}

fn print_from_ptr(adr: ptr, len: uint) {
    write_from_ptr(1, adr, len) ! return
}
