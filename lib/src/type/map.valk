
+ mode Map[T] for HashMap[String, T] {

    + static fn new() SELF {
        return SELF{}
    }

    + macro "{" repeat(items, ws V:v1 ws "=>" ws V:v2 ws optional(",") ws, "}")
    <{
    	let m = SELF.new()
    	#loop macro_values items as item
    	m.set(item.v1, item.v2)
    	#end
    	return m
    }
}
