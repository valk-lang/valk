
use valk:gc
use valk:core

fn __valk_main__() i32 $entrance $parse_last {
    gc:GcShared.init()
    let gc = gc:GcLocal.init()
    
    @allocas

    #loop classes T
    @pool(T) = gc:Pool.init[T]()
    #end

    #loop globals G T
    G = @global_default_value(G)
    #if @is_gc_type(T)
    gc:local_gc.add_global(@ref(G))
    #end
    #end

    co main()
    core:Coro.loop()

    gc.shutdown()
    return 0
}
